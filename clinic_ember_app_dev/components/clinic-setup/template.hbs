
<div class="nonprint">
  <form class="col-md-5" {{action "createClinic" on="submit"}}>
    <div class="form-inline row">
      {{input type=text value=clinic_name placeholder="Clinic Name" class=form-control required="required"}}
      {{input type=text value=clinic_code placeholder="Clinic Code" class=form-control required="required"}}
      {{input type=text value=position placeholder="Position" class=form-control required="required"}}
      <button type="submit" class="btn btn-default">Create Clinic</button>
    </div>
  </form>
</div>



{{yield}}
