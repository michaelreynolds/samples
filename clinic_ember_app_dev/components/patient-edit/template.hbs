<div class="clear"></div>
<form class="form-horizontal nonprint">
  <!-- <div class="form-group">
    <label class="control-label">ID: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.id class="form-control" disabled="disabled"}}
    </div>
  </div> -->
  <div class="form-group">
    <label class="col-sm-2 control-label">First Name: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.first_name class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Last Name: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.last_name class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Mobile Number: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.mobile_number class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Email: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.email class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Address: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.address_1 class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">City: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.city class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">State: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.state class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Zip: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.zip class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Date of Birth: </label>
    <div class="col-sm-10">
      {{input type="text" value=patient.dob class="form-control" required="required"}}
    </div>
  </div>
  <div class="form-inline	form-group">
    <label class="col-sm-2 control-label">Gender: </label>
    <div class="col-sm-10">
      <label class="radio-inline" required>
        {{#radio-button
          value="M"
          required=true
          groupValue=patient.gender}}
          Male
        {{/radio-button}}
      </label>
      <label class="radio-inline" required>
        {{#radio-button
          value="F"
          required=true
          groupValue=patient.gender}}
          Female
        {{/radio-button}}
      </label>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button {{action "edit" patient}} type="submit" class="btn btn-default">Save Patient</button>
    </div>
  </div>
</form>
{{yield}}
